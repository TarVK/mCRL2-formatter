{
    "name": "mcrl2-formatter",
    "version": "0.0.0",
    "private": true,
    "description": "A simple formatter for mcrl2 syntax",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build/**/*.{js,d.ts}"
    ],
    "scripts": {
        "start": "node ./build/index.js",
        "exercise": "node ./build/exercises/run.js",
        "dbg": "node --inspect-brk ./build/index.js",
        "dev": "tsc -w ",
        "build": "tsc",
        "test": "jest -i",
        "test-watch": "jest --watch -i",
        "test-watch-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch --config=\"jest.config.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TarVK/mCRL2-formatter.git"
    },
    "keywords": [
        "mcrl2"
    ],
    "author": "Tar van Krieken",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TarVK/mCRL2-formatter"
    },
    "homepage": "https://github.com/TarVK/mCRL2-formatter",
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.10",
        "@types/parsimmon": "^1.10.6",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "parsimmon": "^1.18.0"
    }
}
